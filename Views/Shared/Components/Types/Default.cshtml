
@{
}
@model IEnumerable<string>
    <div class="d-grid gap-2">

        <a asp-controller="Home" asp-action="Index" asp-route-projectType="" class="btn btn-outline-secondary" asp-route-pageNum="1">Home</a>

        @foreach (string t in Model)
        {
            <a asp-controller="Home" asp-action="Index" asp-route-projectType="@t" 
               class="btn @(ViewBag.SelectedType == t ? "btn-primary": "btn-outline-secondary")" asp-route-pageNum="1">@t</a>
        }
    </div>